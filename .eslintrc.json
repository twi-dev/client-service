{
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "pragma": "h"
    },
    "import/resolver": {
      "babel-module": {}
    }
  },
  "extends": [
    "airbnb",
    "@octetstream"
  ],
  "rules": {
    "import/no-unresolved": 0
  }
}
