    "make": "NODE_ENV=production node build",
    "watch": "NODE_ENV=development node build",
    "babel:make": "babel src -d .",
    "babel:watch": "babel src -w -d .",
    "m": "yarn make",
    "w": "yarn watch",
    "wo": "yarn watch --open",
    "stats": "mkdirp runtime/webpack && NODE_ENV=production webpack --config build --profile --json > runtime/webpack/stats.json",
