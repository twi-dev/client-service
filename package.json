{
  "name": "@twi-project/web-client",
  "author": "Nick K.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "make": "NODE_ENV=production node_modules/.bin/webpack --config build",
    "watch": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config build",
    "babel:make": "node_modules/.bin/babel src -d .",
    "babel:watch": "node_modules/.bin/babel src -w -d .",
    "m": "yarn make",
    "w": "yarn watch",
    "stats": "node_modules/.bin/mkdirp runtime/webpack && NODE_ENV=production node_modules/.bin/webpack --config build --profile --json > runtime/webpack/stats.json",
    "cleanup": "node_modules/.bin/rimraf static/assets/js static/assets/css runtime",
    "lint:eslint": "node_modules/.bin/eslint src build",
    "lint:stylelint": "node_modules/.bin/stylelint --fix",
    "staged": "node_modules/.bin/lint-staged",
    "precommit": "yarn staged"
  },
  "devServer": {
    "port": 1339
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/**/__hook__/**",
      "!test/**/__helper__/**",
      "!test/helper/**"
    ]
  },
